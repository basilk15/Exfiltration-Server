# Example environment for the exfiltration server
# Copy to /etc/default/exfil and adjust values

EXFIL_TOKEN=change-me
MAX_CONTENT_LENGTH_MB=50

# Stealth
EXFIL_JITTER_MIN_MS=100
EXFIL_JITTER_MAX_MS=600
EXFIL_SILENT=0
EXFIL_PATH=/api/v1/collect
EXFIL_SERVER_HEADER=nginx
EXFIL_DISABLE_FILES=1
EXFIL_GET_ENABLE=1
EXFIL_GET_PATH=/pixel.gif
EXFIL_GET_PARAM=q
EXFIL_ALLOWED_HOSTS=exfil.example.com
EXFIL_ALLOWED_REFERERS=https://www.google.com/
EXFIL_ACTIVE_START=1
EXFIL_ACTIVE_END=5
EXFIL_PAD_MIN=0
EXFIL_PAD_MAX=64

# TLS
EXFIL_SSL_CERT=/etc/ssl/certs/exfil.crt
EXFIL_SSL_KEY=/etc/ssl/private/exfil.key
